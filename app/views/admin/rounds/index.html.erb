<h1>Listing rounds</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Created at</th>
  </tr>

<% @rounds.each do |round| %>
  <tr>
    <td><%=h round.id %></td>
    <td><%=h round.created_at %></td>
    <td><%= link_to 'Show', [:admin, round] %></td>
    <td><%= link_to 'Edit', edit_admin_round_path(round) %></td>
    <td><%= link_to 'Destroy', [:admin, round], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New round', new_admin_round_path %>